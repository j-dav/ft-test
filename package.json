{
  "name": "ft-test",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon app.js",
    "test": "jest",
    "lint:fix": "eslint *.js --fix",
    "lint": "eslint *.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-handlebars": "^5.2.0",
    "hbs": "^4.1.1",
    "http-errors": "~1.6.3",
    "jest": "^26.6.3",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.1"
  },
  "devDependencies": {
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3"
  }
}
